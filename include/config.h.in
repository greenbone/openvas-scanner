#define NESSUSD_LOGINS   NESSUSD_STATEDIR "/users"
#define NESSUSD_JOBS	NESSUSD_STATEDIR  "/jobs"
#define NESSUSD_CERTS	NESSUSD_STATEDIR "/certs"

