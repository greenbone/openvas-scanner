    {
      "name": "netbus",
      "description": "NetBus Trojan (Security Alert)",
      "detections": [
        {
          "type": "banner",
          "banner_detection_type": "equals",
          "value": "NetBus",
          "case_sensitive": false
        }
      ],
      "ports": [12345, 12346],
      "save_banner": true,
      "generate_result": {
        "enabled": true,
        "is_vulnerability": true,
        "message": "NetBus backdoor is running on this port"
      },
      "kb_entries": {}
    }
  ]
}
